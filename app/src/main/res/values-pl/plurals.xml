        <item quantity="one">Utwór został usunięty.</item>
        <item quantity="other">Utwory zostały usunięte.</item>
    </plurals>

    <plurals name="number_of_tracks" tools:ignore="MissingQuantity">
        <item quantity="one">%d utwór</item>
