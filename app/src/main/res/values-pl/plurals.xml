        <item quantity="one">%1$d utwór dodano do playlisty \"%2$s\"</item>
        <item quantity="few">%1$d utwory dodano do playlisty \"%2$s\"</item>
        <item quantity="other">%1$d utworów dodano do playlisty "\%2$s\"</item>
    </plurals>
</resources>